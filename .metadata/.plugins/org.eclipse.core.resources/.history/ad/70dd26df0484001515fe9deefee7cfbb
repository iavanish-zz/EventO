package iavanish.Database;

public class Event {

	public long eventID;
	public String name;
	public String date;
	public String time;
	public String rollNo;
	
	public Event(long eventID, String name, String date, String time, String rollNo) {
		this.eventID = eventID;
		this.name = name;
		this.date = date;
		this.time = time;
		this.rollNo = rollNo;
	}
	
}
